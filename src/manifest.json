{
  "manifest_version": 2,
  "name": "Inviduration",
  "version": "1.3",
  "description": "Shows Playlists Duration in Invidio.us instances.",
  "homepage_url": "https://github.com/rsapkf/inviduration",
  "content_scripts": [
    {
      "matches": [
        "*://invidious.snopyta.org/playlist?list=*",
        "*://invidious.kavin.rocks/playlist?list=*",
        "*://*.invidious.xyz/playlist?list=*",
        "*://*.yewtu.be/playlist?list=*",
        "*://*.invidious.tube/playlist?list=*"
      ],
      "js": ["content-script-invidious.js"]
    }
  ]
}
